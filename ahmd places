<script>
    $('.openModal').click(function () {
        var id = $(this).attr('data-id');

        $.ajax({
            type: "POST",
            url: 'https://www.gujarattourism.com/tripplanner/0.1/load_subplace',
            data: 'key=cb784502bffab6e&id=' + id ,

            success: function (data) {
                console.log(data);
                if (data.code == 200) {
                    $("#placeDetails").html(data.html);
                } else {
                    $('#days-error label.error').html(data.msg);
                    $('label.error').show();
                }
            }
        });
    });
</script>

